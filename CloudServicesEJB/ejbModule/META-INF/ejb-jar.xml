	<display-name>CloudServicesEJB</display-name>
	<enterprise-beans>
		<session>
			<ejb-name>CloudMessageService</ejb-name>
			<ejb-class>it.ibm.cs.sample.ejbs.CloudMessageService</ejb-class>
